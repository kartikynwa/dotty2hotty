#!/bin/bash
# DOT_DEST=.xinitrc
xrdb -load ~/.Xresources
~/dotty2hotty/scripterino/wall.sh
unclutter &
start-pulseaudio-x11 &
redshift &
xset -b # turn off beeping
[ -f ~/dotty2hotty/config_files/xmodmaprc ] && xmodmap ~/dotty2hotty/config_files/xmodmaprc
export QT_QPA_PLATFORMTHEME=gtk2
exec dbus-run-session i3
